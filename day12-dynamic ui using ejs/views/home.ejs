<%- include('partials/head') %>
</head>
<body class="bg-gray-100 font-sans">
  <%- include('partials/nav') %>
  <nav>
<main class="container mx-auto bg-white shadow-lg rounded-lg p-8 mt-10 max-w-xl">
  <h2 class="text-2xl text-gray-800 font-bold text-center mb-6">Here are your registered homes:</h2>
  <ul class="space-y-4">
    <% registeredHouses.forEach(home => { %>
      <div>
        <li class="flex items-center justify-between bg-gray-50 hover:bg-gray-100 rounded-lg p-4 shadow transition">
         <img class="w-25 h-25 object-cover rounded-md" src="<%=home.photo%>" alt="<%=home.photo%>">
       </li>
      </div>
      <div>
        
        <li class="flex items-center justify-between bg-gray-50 hover:bg-gray-100 rounded-lg p-4 shadow transition">
          <p class="text-gray-600">House Name:</p><span class="text-lg font-medium text-gray-700"><%= home.houseName %></span>
        </li>
        <li class="flex items-center justify-between bg-gray-50 hover:bg-gray-100 rounded-lg p-4 shadow transition">
          <p class="text-gray-600">Price:</p><span class="text-lg font-medium text-gray-700"><%= home.price %></span>
        </li>
        <li class="flex items-center justify-between bg-gray-50 hover:bg-gray-100 rounded-lg p-4 shadow transition">
          <p class="text-gray-600">Location:</p><span class="text-lg font-medium text-gray-700"><%= home.location %></span>
        </li>
        <li class="flex items-center justify-between bg-gray-50 hover:bg-gray-100 rounded-lg p-4 shadow transition">
        <p class="text-gray-600">Rating:</p><span class="text-lg font-medium text-gray-700"><%= home.rating %></span>
         </li>
      </div>
     
     
    <% }) %>
  </ul>
</main>
  </ul>
  </main>
  </nav>
 
</body>
</html>